{
  "name": "traveler",
  "version": "1.0.0",
  "description": "A worflow for a website for my study",
  "main": "index.js",
  "scripts": {
    "start": "yarn start-app",
    "clear-dist": "rm -rf dist/",
    "build-ts": "yarn clear-dist && tsc",
    "build": "yarn build-ts && babel --copy-files --include node_modules --out-dir dist dist",
    "serve": "nodemon --delay 3 --ignore \"test\" --watch ./dist --exec babel-node dist/server.js || exit 1",
    "start-app": "yarn build-ts && concurrently \"tsc -w\" \"(sleep 3 && yarn serve)\" --raw"
  },
  "private": true,
  "author": "Maikel van Veen",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.11.1",
    "@types/mongoose": "^5.0.7",
    "@types/node": "^9.4.6",
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^3.1.0",
    "concurrently": "^3.5.1",
    "copyfiles": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.1.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.4",
    "nodemon": "^1.17.1",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^5.1.0",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "dotenv": "^5.0.1",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.2.0",
    "mongodb": "^3.0.4",
    "mongoose": "^5.0.10",
    "multer": "^1.3.0"
  }
}
